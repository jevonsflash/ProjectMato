<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MusicMink">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <LinearGradientBrush x:Key="PlayerControlGradientColor1" EndPoint="0.5,1" StartPoint="0.5,0">
            </LinearGradientBrush>
            <LinearGradientBrush x:Key="PlayerControlGradientColor2" EndPoint="0.5,1" StartPoint="0.5,0">
            </LinearGradientBrush>
            <SolidColorBrush x:Key="PlayerControlForegroundColor1" Color="Black" />
            <SolidColorBrush x:Key="PlayerControlForegroundColor2" Color="Black" />
            <ImageBrush x:Key="MainBackGroundBrush" ImageSource="Img/Mato.png"></ImageBrush>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

</ResourceDictionary>
